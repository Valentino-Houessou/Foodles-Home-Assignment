  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "dev": "cd dist/src && NODE_ENV=development node index.js",
    "dev2": "cd src && NODE_ENV=development nodemon --exec ts-node index.ts -e ts,graphql",
    "test-server": "cd src && NODE_ENV=test ts-node index.ts",
    "start": "cd src && NODE_ENV=production ts-node index.ts",
    "testdb:setup": "NODE_ENV=test ts-node ./test/utils/setup.ts",
    "test": "npm run testdb:setup && jest -c jest.config.js --detectOpenHandles",
    "test:watch": "npm run testdb:setup && jest -c jest.config.js --detectOpenHandles --watchAll"
  },
